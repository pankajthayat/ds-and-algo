package singlyLinkedList;

public class Main {
public static void main(String[] args) {
	SinglyLinkedList list = new SinglyLinkedList();
	
	list.insertAtEnd(1);
	list.insertAtEnd(2);
	list.insertAtEnd(3);
	list.insertAtEnd(7);
	list.insertAtBeginning(90);
	list.display();
	System.out.println();
	list.insert(11, 1);
	list.display();
	list.insert(22, 7);
//	System.out.println();
	list.display();
//	list.insert(99, 5);
//	list.display();
}
}
