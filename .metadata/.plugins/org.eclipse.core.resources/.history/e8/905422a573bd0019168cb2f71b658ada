package singlyLinkedList;

public class SinglyLinkedList {
	private Node head;
	
	
	public void insert(int value) {
		if(head==null) {
			head = new Node();
			head.setNext(head)
		}else {
			Node current = head;
			while(current !=null) {
				current = current.getNode();
			}
			
			current =new Node();
			current.setValue(value);
		}
	}
	
	public void display() {
		Node current = head;
		while(current !=null) {
			System.out.println(current.getValue()+", ");
			current = current.getNode();
		}
	}
}
