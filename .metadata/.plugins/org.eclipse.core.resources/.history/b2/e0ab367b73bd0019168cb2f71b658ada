package singlyLinkedList;

public class Node {
	private int value;
	private Node node;
	public int getValue() {
		return value;
	}
	public Node setValue(int value) {
		this.value = value;
		return this;
	}
	public Node getNode() {
		return node;
	}
	public Node setNode(Node node) {
		this.node = node;
		return this;
	}
	
}
